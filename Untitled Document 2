<!DOCTYPE html>
<html>
<head>
  <link href='http://fonts.googleapis.com/css?family=Roboto:100' rel='stylesheet' type='text/css'>
  
<script type='text/javascript'>
	var imageID=0;
	function changeimage(every_seconds){
    		
    	if(!imageID){
        	document.getElementById("myimage").src="http://hushcoffeebar.com/wp-content/uploads/2013/08/HUSH-Coffee-Bar-3d-3.jpg";
        imageID++;
    	}
    
	else{if(imageID==1){
        document.getElementById("myimage").src="http://hushcoffeebar.com/wp-content/uploads/2013/08/HUSH-Coffee-Bar-3d-1.jpg";
        imageID++;
    

	}else{if(imageID==2){
        document.getElementById("myimage").src="http://hushcoffeebar.com/wp-content/uploads/2013/08/HUSH-Coffee-Bar-3d-5.jpg";
        imageID=0;
    
	}}}
    	setTimeout("changeimage("+every_seconds+")",((every_seconds)*1500));

	
	}

</script>
</head>

<body style='background:black;' onload='changeimage(2)'>
<div style='position:absolute;width:100%;height:100%;left:0px;top:0px;' align='center'><img width='1700px' height='800px' id='myimage' src='http://hushcoffeebar.com/wp-content/uploads/2013/08/HUSH-Coffee-Bar-3d-3.jpg'/></div>
</body></html>
  

